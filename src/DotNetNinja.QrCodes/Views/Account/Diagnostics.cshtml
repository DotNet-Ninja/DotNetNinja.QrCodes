@model IEnumerable<System.Security.Claims.Claim>
@{
    ViewData[ViewKey.Title] = "Profile";
}

<h1>Claims</h1>
<table class="table table-striped table-borderless">
    <thead>
        <tr>
            <th>Claim</th>
            <th>Value</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var claim in Model)
        {
            <tr>
                <td>@claim.Type</td>
                <td>@claim.Value</td>
            </tr>
        }
    </tbody>
</table>
<avatar size="128" />
